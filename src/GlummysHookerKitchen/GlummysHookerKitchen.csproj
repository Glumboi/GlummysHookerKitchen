    <ItemGroup>
      <Reference Include="System.Windows.Forms">
        <HintPath>C:\Program Files\dotnet\shared\Microsoft.WindowsDesktop.App\6.0.28\System.Windows.Forms.dll</HintPath>
      </Reference>
    </ItemGroup>

